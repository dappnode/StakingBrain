{
  "name": "@stakingbrain/ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.4",
    "@mui/material": "^5.2.3",
    "@mui/system": "^5.4.1",
    "@mui/x-data-grid": "^5.1.0",
    "@stakingbrain/common": "^0.1.0",
    "history": "5",
    "lodash-es": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-router-dom": "6",
    "react-scripts": "^5.0.1",
    "socket.io-client": "^4.5.4",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.tsx src/**/*.ts",
    "format": "eslint src/**/*.tsx src/**/*.ts --fix",
    "pretty": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/tests/*/*.ts'",
    "start:dev": "nodemon --ignore \"*.json\" -e ts,tsx --watch src/ --exec \"yarn build\"",
    "start:webpack:dev": "nodemon --ignore \"*.json\" -e ts,tsx --watch src/ --exec \"yarn build\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-flow": "^7.23.3",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@types/chai": "^4.3.3",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "chai": "^4.3.6",
    "eslint": "^8.33.0",
    "mocha": "^10.0.0",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1"
  }
}
